        13 HD3     HC          0.0600
        14 NE      NY         -0.6200
        15 HE      H           0.3800
        16 CZ      CAr         0.5500
        17 NH1     NZ         -0.7850
        18 HH11    H           0.3800
        19 NH2     NY         -0.7850
        20 HH21    H           0.3600
        21 HH22    H           0.3600
        22 C       C           0.5000
        23 O       O          -0.5000
    [bonds]
       N    H
       N    CA
       CA   HA
       CA   CB
       CB   HB2
       CB   HB3
       CB   CG
