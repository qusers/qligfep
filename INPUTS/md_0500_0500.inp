[MD]
steps                     NSTEPS2
stepsize                  STEPSIZE
temperature               T_VAR
bath_coupling             10
shake_hydrogens           STEPTOGGLE
shake_solute              STEPTOGGLE
shake_solvent             on
lrf                       on

[cut-offs]
solute_solvent            10
solute_solute             10
solvent_solvent           10
q_atom                    99
lrf                       99

[sphere]
shell_force               10.0
shell_radius              SPHERE

[solvent]
radial_force              60.0
polarisation              on
polarisation_force        20.0

[intervals]
output                    5
energy                    10
trajectory                100
non_bond                  25

[files]
topology                  dualtop.top
trajectory                md_0500_0500.dcd
restart                   eq5.re
energy                    md_0500_0500.en
final                     md_0500_0500.re
fep                       FEP_VAR

[trajectory_atoms]
not excluded

[lambdas]
0.500 0.500

[sequence_restraints]
WATER_RESTRAINT

[distance_restraints]
